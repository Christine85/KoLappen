    { @* START NAVBAR USER *@
        <div class="row">
            <div class="col-md-10 col-md-offset-1">

                <nav class="navbar navbar-default">
                    <div class="container-fluid">
                        <div class="navbar-header">
                            <a class="navbar-brand" href="~/home/index"><span class="glyphicon glyphicon-home" id="settingsIcon"></span></a>
                        </div>
                        <ul class="nav navbar-nav">
                            <li class="dropdown">
                                <a class="dropdown-toggle" data-toggle="dropdown" href="~/profile/profile">Mina sidor<span class="caret"></span></a>
                                <ul class="dropdown-menu">
                                    <li><a href="~/profile/profile">Profil</a></li>
                                    <li><a href="~/profile/editprofile">Ändra profil</a></li>
                                </ul>
                            </li>
                            <li><a href="~/home/calendar">Kalender</a></li>
                            <li><a href="~/">Min klass</a></li>
                            <li><a href="#">Meny 3</a></li>
                            <li><a href="#">Meny 3</a></li>
                            <li><a href="#">Inte admin som är inne</a></li>
                        </ul>
                        <ul class="nav navbar-nav navbar-right">
                            <li class="dropdown">
                                <a href="~/home/calendar" class="dropdown-toggle" data-toggle="dropdown"><span class="glyphicon glyphicon-calendar" id="settingsIcon"></span></a>
                                <ul class="dropdown-menu">
                                    <li><a href="~/home/calendar"><iframe src="https://calendar.google.com/calendar/embed?title=AWA%20VT16%20-%20C%23%20.NET&amp;showTitle=0&amp;showNav=0&amp;showDate=0&amp;showPrint=0&amp;showTabs=0&amp;showCalendars=0&amp;showTz=0&amp;height=200&amp;wkst=2&amp;hl=sv&amp;bgcolor=%23FFFFFF&amp;src=qd4g3h2qi5vh2t873katrenqko%40group.calendar.google.com&amp;color=%23060D5E&amp;src=en.swedish%23holiday%40group.v.calendar.google.com&amp;color=%23711616&amp;src=e_2_en%23weeknum%40group.v.calendar.google.com&amp;color=%23125A12&amp;ctz=Europe%2FStockholm" style="border-width:0" width="267" height="200" frameborder="0" scrolling="no"></iframe></a></li>
