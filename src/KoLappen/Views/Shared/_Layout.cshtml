    @* main innehållet *@
    <div class="row">
        <div class="col-md-8 col-md-offset-1">

            @RenderBody()

        </div>

        @* kölappen *@
        <div class="row">
            <div class="col-md-2" id="theQLappBackground">

                @*Här displayas infon för köplappen*@
                <div id="Queue">
                </div>

                <br /><br /><br /><br /><br /><br /><br /><br />
            </div>
        </div>
    </div>


    @*<div class="panel-footer">HOME OF THE YOUNG PROFESSIONALS</div>*@

    <script type="text/javascript">

        //Funktion för knapptrycken
        function updateNeedHelp(isEqueue) {
            var action = isEqueue ? "EnQueue" : "DeQueue";
            $.post("/Queuelist/" + action, function (html) {
                $("#Queue").html(html);
            });
        }

        //Uppdaterar Köplappen inom ett visst tidintervall (kallar på Controllerna/Action för att hämta listan)
        var refreshQueue = function () {
            $.get("/Queuelist/GetQueuelist", function (html) {
                $("#Queue").html(html);
            });
        };
        refreshQueue();
        window.setInterval("refreshQueue()", 30000);

    </script>

</body>


</html>




